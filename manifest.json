{
  "manifest_version": 2,

  "name": "ChromeMusicInfo",
  "description": "This extension get music info from supported sites and makes it available to rainmeter",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/music/*"
      ],
      "js": [
        "musicInfoScriptGPM.js"
      ]
    },
    {
      "matches": [
        "https://soundcloud.com/*"
      ],
      "js": ["musicInfoScriptSC.js"]
    }
  ]
}